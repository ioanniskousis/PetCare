<p id="notice"><%= notice %></p>

<h1>Found Photos</h1>

<table>
  <thead>
    <tr>
      <th>Found</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @found_photos.each do |found_photo| %>
      <tr>
        <td><%= found_photo.found_id %></td>
        <td><%= link_to 'Show', found_photo %></td>
        <td><%= link_to 'Edit', edit_found_photo_path(found_photo) %></td>
        <td><%= link_to 'Destroy', found_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Found Photo', new_found_photo_path %>
